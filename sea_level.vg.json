{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/cccovo/3179_testrepo/main/sea%20level.csv"
    },
    "width":350,
    "height":300,
    "title":{
        "text": "Changing Sea Level from 1880 to 2019 | inches",
        "fontSize":14
    },
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Year",
        "type": "temporal",
        "axis": {"title": "", "grid": false}
      },
      "y": {
          "field": "Adjusted sea level (inches)",
          "type": "quantitative",
          "axis": {"grid": false}
      },
      "tooltip":[
          {"field": "Year", "type": "temporal"},
          {"field": "Adjusted sea level (inches)", "type": "quantitative"}
      ]
    }
  }

